@model IEnumerable<ContactContent>


@foreach (ContactContent content in Model)
{
    <div class="contact-icon">
        <img src="~/assets/img/contact/@content.Icon" alt="contact">
    </div>
    <div class="contact-add">
        <h3>@content.Title</h3>
        <p>@content.MainDescription</p>
        <p>@content.SubDescription</p>
    </div>

}
